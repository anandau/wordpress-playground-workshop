{
  "$schema": "https://playground.wordpress.net/blueprint-schema.json",
  "meta": {
    "title": "Gaming News",
    "description": "A gaming news site created with the Spiel theme.",
    "author": "WordPress.com",
    "categories": [
      "Website",
      "News"
    ]
  },
  "login": true,
  "siteOptions": {
    "blogname": "Spiel"
  },
  "steps": [
    {
      "step": "resetData"
    },
    {
      "step": "installTheme",
      "themeData": {
        "resource": "bundled",
        "path": "./spiel.zip"
      },
      "options": {
        "activate": true
      }
    },
    {
      "step": "importWxr",
      "file": {
        "resource": "bundled",
        "path": "./content.xml"
      }
    }
  ],
  "preferredVersions": {
    "php": "8.3",
    "wp": "latest"
  },
  "features": {}
}